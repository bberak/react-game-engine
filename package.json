  "bugs": {
    "url": "https://github.com/bberak/react-game-engine/issues"
  },
  "homepage": "https://github.com/bberak/react-game-engine",
  "peerDependencies": {
    "react": ">= 16.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.2.5",
    "@types/react": "^18.2.8",
    "react": "^16.3.0",
    "typescript": "^5.1.0",
    "vite": "latest",
    "vite-plugin-dts": "^2.3.0"
  }
